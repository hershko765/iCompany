<script type="text/html" >
    <@ if(checkedColumn) { @>
    <td style="width: 53px">
        <label style="margin-left: 24px; margin-top: -7px">
            <input type="checkbox" class="ace datagrid-checkbox"  />
            <span class="lbl"></span>
        </label>
    </td>
    <@ } @>
    <@ _.each(rawData, function(raw){ @>
    <td data-id="{{ raw.name }}" >{{ raw.title }}</td>
    <@ }); @>
    <td>
        <div class="visible-md visible-lg hidden-sm hidden-xs btn-group action-buttons">
            <div class="btn-group">
            <@ _.each(actions, function(action){ @>
                <button type="button" data-trigger="{{ action.execute }}" class="btn btn-{{ action.type }} btn-sm actionButton"><span class="fa fa-{{ action.icon }}"></span></button>
            <@ }); @>
            </div>
        </div>
    </td>
</script>